<html>
    <body>
        <script>
            class Car{
                    constructor(make, speed){
                        this.make = make;
                        this.speed = speed;
                        console.log(`Make: ${make}`)
                        console.log(`Speed: ${speed}`)
                    }
                    
                    calculateacceleration(){
                        return this.speed + 10 }
}
        </script>
    </body>
